<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.tax.pojo.nsfw.RolePrivilege" table="role_privilege">
		<composite-id name="id" class="com.tax.pojo.nsfw.RolePrivilegeId">
			<key-property name="roleId" type="string">
				<column name="role_id" not-null="true"></column>
			</key-property>
			<key-property name="code" type="string">
				<column name="code" not-null="true"></column>
			</key-property>
		</composite-id>
	</class>
</hibernate-mapping>